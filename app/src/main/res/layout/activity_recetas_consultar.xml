<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
<LinearLayout android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/RecetasMedicas"/>
    <Spinner
        android:id="@+id/spinnerConsultarRecetas"
        android:layout_width="fill_parent"
        android:importantForAutofill="no"
        android:inputType="text"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"
         android:layout_height="25dp"/>
    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/btnConsultarRecetas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btnConsultarRecetas"
            android:onClick="consultarReceta"/>
    </LinearLayout>
    <TextView
        android:text="@string/RecetaMedicas"
        android:textSize="16sp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:textStyle="bold" />
    <TextView
        android:text="@string/idRecetaMedica"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"/>
    <EditText
        android:id="@+id/editIdRecetaMedica"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"/>
    <TextView
        android:text="@string/numeroReceta"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />

    <EditText
        android:id="@+id/editNumeroReceta"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck" />
    <TextView
        android:text="@string/fechaReceta"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />

    <EditText
        android:id="@+id/editFechaReceta"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="HardcodedText,TouchTargetSizeCheck,SpeakableTextPresentCheck" />
    <TextView
        android:text="@string/detallesMedico"
        android:textSize="16sp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:padding="2dp"
        android:textStyle="bold"
        />
    <TextView
        android:text="@string/idMedico"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"/>
    <EditText
        android:id="@+id/editIdMedico"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"
        />
    <TextView
        android:text="@string/nombreMedico"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />

    <EditText
        android:id="@+id/editNombreMedico"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck" />
    <TextView
        android:text="@string/apellidoMedico"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />
    <EditText
        android:id="@+id/editApellidoMedico"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"
        />
    <TextView
        android:text="@string/especialidadMedico"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />
    <EditText
        android:id="@+id/editEspecialidadMedico"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"
        />
    <TextView
        android:text="@string/jvpmMedico"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />
    <EditText
        android:id="@+id/editJVPM"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"
        />
    <TextView
        android:text="@string/detallesReceta"
        android:textSize="16sp"
        android:gravity="center_horizontal"
        android:padding="2dp"
        android:textStyle="bold"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:text="@string/idDetalleReceta"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />
    <EditText
        android:id="@+id/editIdDetalleReceta"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"
        />
    <TextView
        android:text="@string/consultaMedicamentoReceta"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />
    <EditText
        android:id="@+id/readMedicamentoReceta"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"
        />
    <TextView
        android:text="@string/dosisReceta"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />
    <EditText
        android:id="@+id/editDosisReceta"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"
        />
    <TextView
        android:text="@string/periodicidadReceta"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />
    <EditText
        android:id="@+id/editPeriodicidadReceta"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="LabelFor,TouchTargetSizeCheck,SpeakableTextPresentCheck"
        />
    <TextView
        android:text="@string/fechaInicioTratamiento"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />
    <EditText
        android:id="@+id/editFechaInicioTratamiento"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="HardcodedText,TouchTargetSizeCheck"
        />
    <TextView
        android:text="@string/fechaFinTratamiento"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" />
    <EditText
        android:id="@+id/editFechaFinTratamiento"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:enabled ="false"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:cursorVisible="false"
        android:longClickable="false"
        tools:ignore="HardcodedText,TouchTargetSizeCheck"
        />
</LinearLayout>
</ScrollView>